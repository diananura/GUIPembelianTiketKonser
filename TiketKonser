    private void rb_saturdayActionPerformed(java.awt.event.ActionEvent evt) {                                            
        // TODO add your handling code here:
         //menampilkan harga setelah pilih kategori
        if(rb_saturday.isSelected()){
            txt_harga.setText(String.valueOf(saturday));
        }
    }                                           

    private void txt_nomorActionPerformed(java.awt.event.ActionEvent evt) {                                          
        // TODO add your handling code here:
    }                                         

    private void txt_bayarActionPerformed(java.awt.event.ActionEvent evt) {                                          
        // TODO add your handling code here:
        //menginput jumlah uang dan mengetahui total kembalian
        bayar=Integer.parseInt(txt_bayar.getText());
        kembalian=Integer.parseInt(txt_bayar.getText());
        total=bayar-kembalian;
        txt_kembalian.setText(String.valueOf(total));
    }                                         

    private void btn_keluarActionPerformed(java.awt.event.ActionEvent evt) {                                           
        // TODO add your handling code here:
        int answer=JOptionPane.showConfirmDialog(null,"Anda Yakin Keluar","Keluar",JOptionPane.OK_OPTION);
        if(answer==JOptionPane.OK_OPTION){
            dispose();
        }
    }                                          

    private void kategoriActionPerformed(java.awt.event.ActionEvent evt) {                                         
        // TODO add your handling code here:
         //membuat kondisi untuk menentukan harga sesuai kategori
        if(kategori.getSelectedItem().equals("Pilih Kategori")) {
            buttonGroup1.clearSelection();
            txt_harga.setText("");
        }
        else if(kategori.getSelectedItem().equals("VIP")){
            saturday=500000;
            sunday=500000;
            days=750000;
        }
        else if(kategori.getSelectedItem().equals("FESTIVAL")){
            saturday=300000;
            sunday=300000;
            days=500000;
        }
    }                                        

    private void rb_sundayActionPerformed(java.awt.event.ActionEvent evt) {                                          
        // TODO add your handling code here:
         if(rb_sunday.isSelected()){
            txt_harga.setText(String.valueOf(sunday));
        }
    }                                         

    private void rb_daysActionPerformed(java.awt.event.ActionEvent evt) {                                        
        // TODO add your handling code here:
        if(rb_days.isSelected()){
            txt_harga.setText(String.valueOf(days));
        }
    }                                       

    private void txt_jumlahActionPerformed(java.awt.event.ActionEvent evt) {                                           
        // TODO add your handling code here:
         //menginput jumlah tiket dan mengetahui total pembayaran
        beli=Integer.parseInt(txt_jumlah.getText());
        harga=Integer.parseInt(txt_harga.getText());
        total=bayar*harga;
        txt_total.setText(String.valueOf(total));
    }                                          

    private void txt_jumlahKeyReleased(java.awt.event.KeyEvent evt) {                                       
        // TODO add your handling code here:
        int a,b,c;
        a=Integer.valueOf(txt_harga.getText());
        b=Integer.valueOf(txt_jumlah.getText());
        c=a*b;
        txt_total.setText(""+c);
    }                                      

    private void txt_bayarKeyReleased(java.awt.event.KeyEvent evt) {                                      
        // TODO add your handling code here:
        int a,b,c;
        a=Integer.valueOf(txt_bayar.getText());
        b=Integer.valueOf(txt_total.getText());
        c=a-b;
        txt_kembalian.setText(""+c);
    }                                     

    private void btn_inputActionPerformed(java.awt.event.ActionEvent evt) {                                          
        // TODO add your handling code here:
        buttonGroup1.clearSelection();
        txt_harga.setText("");
        kategori.setSelectedItem("Pilih Kategori");
        txt_nomor.setText("");
        txt_nama.setText("");
        txt_total.setText("");
        txt_jumlah.setText("");
        txt_bayar.setText("");
        txt_kembalian.setText("");
    }                                         

    private void btn_cetakActionPerformed(java.awt.event.ActionEvent evt) {                                          
        // TODO add your handling code here:
        btn_cetak.setText((String)kategori.getSelectedItem());
        txt_area.setText(
                "Nomor Pesanan          :"+txt_nomor.getText()+"\n"+
                "Nama                   :"+txt_nama.getText()+"\n"+
                "Jumlah Beli            :"+txt_jumlah.getText()+"\n"+
                "Total Bayar            :"+txt_total.getText()+"\n"+        
                "Uang Bayar             :"+txt_bayar.getText()+"\n"+     
                "Uang Kembalian         :"+txt_kembalian.getText()+"\n");
    }                                         

